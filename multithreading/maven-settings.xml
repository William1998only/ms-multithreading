<settings>
	<servers>
		<server>
			<id>nexus</id>
			<username>${nexusUser}</username>
			<password>${nexusPassword}</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>default</id>
			<properties>

				<nexus-release>nexus</nexus-release>
				<nexus-release-url>https://nexus.corp.bankbtpn.co.id/repository/maven-commontribe-releases/</nexus-release-url>
				<nexus-snapshot>nexus</nexus-snapshot>
				<nexus-snapshot-url>https://nexus.corp.bankbtpn.co.id/repository/maven-commontribe-snapshots/</nexus-snapshot-url>

			</properties>
			<repositories>
				<repository>
					<id>central</id>
					<name>central</name>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
				</repository>
				<repository>
					<id>spring-releases</id>
					<name>spring-releases</name>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
				</repository>
				<repository>
					<id>spring-ext</id>
					<name>spring-ext</name>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
				</repository>
				<repository>
					<id>spring-milestones</id>
					<name>spring-milestones</name>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
				</repository>
				<repository>
					<id>spring-snapshots</id>
					<name>spring-snapshots</name>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<name>central</name>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
					<!-- <url>http://central.maven.org/maven2/</url>  -->
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>spring-releases</id>
					<name>spring-releases</name>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>spring-ext</id>
					<name>spring-ext</name>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>spring-milestones</id>
					<name>spring-milestones</name>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>spring-snapshots</id>
					<name>spring-snapshots</name>
					<url>https://nexus.corp.bankbtpn.co.id/repository/maven-public/</url>
					<releases><enabled>true</enabled></releases>
					<snapshots><enabled>true</enabled></snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>

	</profiles>
	
